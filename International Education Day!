import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here


      Scanner sc=new Scanner(System.in);
      int A=sc.nextInt();
      int B=sc.nextInt();
      int C=sc.nextInt();
      int sum1=A*C;
      int sum2=B*C;
      if(sum1>sum2){
          System.out.println(sum1);
      }
      else{
          System.out.println(sum2);
    
      }

	}
}
